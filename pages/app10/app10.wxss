/* pages/domain/domain.wxss */
page{
  background-image: url("https://z3.ax1x.com/2021/04/20/c7AFNq.png");
  background-size: 100% 100%;
}
swiper{
  width: 100%;
  height: 330rpx;
}
image{
  width: 100%;
}
.testview{
  background-image: url("https://z3.ax1x.com/2021/04/20/c7Vyng.png");
  width: 100%;
  height: 150rpx;
  background-size: 100% 100%;
  }

.domain{
  text-align: left;
  width:100%;
  height:120rpx;
}
.input{
  background-color: rgba(242, 243, 233, 0.945);
  border:1px solid rgba(250, 252, 249, 0.993);
  width: 100%;
  height: 100%;
  border-radius: 10px;
}
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.7;
 }
  
 .modalDlg{
  width: 580rpx;
  height: 580rpx;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  margin: -370rpx 85rpx;
  background-color: #fff;
  border-radius: 36rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
 }
 .error{
   background-image: url(https://z3.ax1x.com/2021/04/20/c7FLSU.png);
   width: 200px;
   height: 200px;
   background-size: 100% 100%;
 }
.go{
  position: relative;
}